{
  "name": "@sparkdesignsystem/spark-react",
  "version": "6.0.0",
  "description": "A collection of Spark Design System components in React 16+",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sparkdesignsystem/spark-design-system"
  },
  "scripts": {
    "test": "jest --config ./src/jest-config.js --coverage --verbose",
    "test:watch": "jest --config ./src/jest-config.js --coverage --verbose --watch",
    "test:generate-output": "jest --config ./src/jest-config.js --json --outputFile=./src/.jest-test-results.json",
    "build": "rollup -c rollup.config.js",
    "build-css": "node-sass ./_spark.scss web/css/spark.min.css --output-style compressed",
    "build-styles": "cd '../styles/' && npm install && node build.js && npm run build-css",
    "storybook": "npm run prebuild:storybook && npm run prebuild:storybook && start-storybook -p 6006 -s ../storybook-utilities/favicon",
    "sassdocmodifiers": "sassdoc '../styles/**/_*.scss' --parse > ../src/data/sass-modifiers.json",
    "prebuild:storybook": "npm run build-styles && npm run sassdocmodifiers && npm run test:generate-output",
    "build-storybook": "npm run prebuild:storybook && build-storybook -s ../storybook-utilities/favicon",
    "prepublishOnly": "npm run build"
  },
  "author": "Quicken Loans",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-google-analytics": "^5.3.18",
    "@storybook/addon-jest": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@storybook/theming": "^5.3.18",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.9.0",
    "node-sass": "^4.13.1",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "regenerator-runtime": "^0.13.5",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "sass-loader": "^8.0.2",
    "sassdoc": "^2.7.1",
    "sinon": "^7.5.0"
  },
  "dependencies": {
    "@sparkdesignsystem/spark-styles": "^3.0.0",
    "classnames": "^2.2.6",
    "focus-visible": "5.0.2",
    "lodash.assign": "^4.2.0",
    "lodash.uniqueid": "^4.0.1",
    "prop-types": "^15.6.2",
    "react-animate-height": "^2.0.20",
    "tiny-date-picker": "github:sparkdesignsystem/tiny-date-picker#v3.2.9",
    "warning": "^4.0.3"
  }
}
