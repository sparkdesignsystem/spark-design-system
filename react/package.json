{
  "name": "@sparkdesignsystem/spark-react",
  "version": "1.4.0-alpha.0",
  "description": "A collection of Spark Design System components in React 16+",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --config ./src/jest-config.js --coverage",
    "test:generate-output": "jest --config ./src/jest-config.js --json --outputFile=./src/.jest-test-results.json",
    "sassdocmodifiers": "sassdoc '../html/**/_*.scss' --parse > ../src/data/sass-modifiers.json",
    "build": "rollup -c rollup.config.js",
    "storybook": "npm run prebuild:storybook && start-storybook -p 6006",
    "prebuild:storybook": "npm run test:generate-output && npm run sassdocmodifiers",
    "build-storybook": "npm run prebuild:storybook && build-storybook",
    "prepublishOnly": "npm run build"
  },
  "author": "Quicken Loans",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "@sparkdesignsystem/spark": "^12.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@sparkdesignsystem/spark": "^12.0.0",
    "@sparkdesignsystem/spark-react": "1.1.1",
    "@storybook/addon-a11y": "^5.3.0-rc.12",
    "@storybook/addon-docs": "^5.3.0-rc.12",
    "@storybook/addon-jest": "^5.3.0-rc.12",
    "@storybook/addons": "^5.3.0-rc.12",
    "@storybook/react": "^5.3.0-rc.12",
    "@storybook/theming": "^5.3.0-rc.12",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^1.23.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "sass-loader": "^8.0.0",
    "sassdoc": "^2.7.1",
    "sinon": "^7.5.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash.assign": "^4.2.0",
    "lodash.uniqueid": "^4.0.1",
    "prop-types": "^15.6.2",
    "react-animate-height": "^2.0.16",
    "tiny-date-picker": "^3.2.8",
    "warning": "^4.0.3"
  }
}
