{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "sprk",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "sprk",
      "kebab-case"
    ],
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "ordered-imports": false
  }
}
